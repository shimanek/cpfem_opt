echo "Running program..."
module load abaqus
module load intel
abaqus job=UT_singleCubic_010_BW user=umatcrystal_mod_XIT.f cpus=1 double int ask_delete=OFF
# abaqus cae nogui=extract_allSDV.py 
# abaqus cae nogui=extract_SS_singleEl.py
echo " "
echo "Job completed at `date`"
echo " "
diff=$SECONDS
echo "$(($diff / 3600)) hours, $((($diff / 60) % 60)) minutes and $(($diff % 60)) seconds elapsed."
